    "@babel/runtime-corejs2": "7.6.0",
    "@babel/template": "7.6.0",
    "@taskr/babel": "1.1.0-next.1",
    "@taskr/clear": "1.1.0",
    "@taskr/esnext": "1.1.0",
    "@taskr/watch": "1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-react-require": "3.1.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "mkdirp": "0.5.1",
