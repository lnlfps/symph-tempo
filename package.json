{
  "name": "@symph/tempo",
  "version": "1.2.3",
  "description": "MVC React app framework, based on Redux",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": "https://github.com/lnlfps/symph-tempo",
  "homepage": "https://github.com/lnlfps/symph-tempo",
